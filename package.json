{
  "name": "react-vectorgrid-leaflet",
  "version": "0.1.0-alpha.1",
  "description": "React wrapper for Leaflet.VectorGrid",
  "main": "index.ts",
  "repository": "git@github.com:Rennzie/react-vectorgrid-leaflet.git",
  "author": "Sean.Rennie <rnnsea001@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf cjs esm dist",
    "build": "yarn clean && yarn build:cjs && yarn build:types && yarn build:umd && yarn build:umd:min",
    "build:cjs": "babel src --out-dir cjs --extensions .ts,.tsx",
    "build:types": "tsc --emitDeclarationOnly",
    "build:umd": "BABEL_ENV=rollup NODE_ENV=development rollup -c",
    "build:umd:min": "BABEL_ENV=rollup NODE_ENV=production rollup -c",
    "start": "rollup -c -w",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
    "prettier": "prettier --ignore-path .eslintignore --write \"./src/**/*.+(js|ts|json)\" ",
    "format": "yarn prettier --write ",
    "check-format": "yarn prettier --list-different ",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint"
  },
  "dependencies": {
    "@types/leaflet": "^1.5.21",
    "leaflet.vectorgrid": "^1.3.0"
  },
  "peerDependencies": {
    "@react-leaflet/core": "^1.0.2",
    "leaflet": "^1.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "leaflet": "^1.7.1",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-leaflet": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.1.3"
  }
}
